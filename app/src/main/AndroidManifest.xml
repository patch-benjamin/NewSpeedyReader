<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.benjaminpatch.newspeedyreader" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".levels"
            android:label="@string/title_activity_levels" >
        </activity>
        <activity
            android:name=".readingAssistant"
            android:label="@string/title_activity_reading_assistant" >
        </activity>
        <activity
            android:name=".userList"
            android:label="@string/title_activity_user_list" >
        </activity>
        <activity
            android:name=".newUser"
            android:label="@string/title_activity_new_user" >
        </activity>
        <activity
            android:name=".bookList"
            android:label="@string/title_activity_book_list" >
        </activity>
        <activity
            android:name=".newBook"
            android:label="@string/title_activity_new_book" >
        </activity>
    </application>

</manifest>
